<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - to na área</title>
    <link rel="icon" href="@Url.Content("~/Images/location256.png")">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="@Url.Action("Index", "Home")" class="navbar-brand" style="background-color: white;">
                    <img src="@Url.Content("~/Images/location32.png")" style="margin-top: -5px;" />
                </a>
                <a href="@Url.Action("Index", "Home")" class="navbar-brand">&nbsp;&nbsp;ToNaArea</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
                @Html.Partial("LoginHeader")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>
                &copy; @DateTime.Now.Year - ToNaArea
                &nbsp;
                <a href="http://fb.com/eduardospaki" target="_blank">
                    <img src="@Url.Content("~/Images/fb.png")" />
                </a>
                &nbsp;
                <a href="http://twitter.com/eduardospaki" target="_blank">
                    <img src="@Url.Content("~/Images/twitter.png")" />
                </a>
                &nbsp;
                <a href="https://br.linkedin.com/in/eduardospaki" target="_blank">
                    <img src="@Url.Content("~/Images/linkedin.png")" />
                </a>
            </p>
        </footer>
    </div>

